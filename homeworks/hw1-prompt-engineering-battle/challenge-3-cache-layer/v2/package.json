{
  "name": "typescript-cache-ttl-lru",
  "version": "1.0.0",
  "description": "TypeScript caching layer with TTL, LRU eviction, and file persistence",
  "main": "dist/cache.js",
  "types": "dist/cache.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "cache",
    "ttl",
    "lru",
    "typescript",
    "persistence"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.ts",
      "**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "cache.ts"
    ]
  }
}